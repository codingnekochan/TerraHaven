            "/Users/mimi/Documents/PORTFOLIO PROJECTS/TerraHaven/node_modules/@expo-google-fonts/lato/100Thin_Italic/Lato_100Thin_Italic.ttf",
            "/Users/mimi/Documents/PORTFOLIO PROJECTS/TerraHaven/node_modules/@expo-google-fonts/lato/300Light/Lato_300Light.ttf",
            "/Users/mimi/Documents/PORTFOLIO PROJECTS/TerraHaven/node_modules/@expo-google-fonts/lato/300Light_Italic/Lato_300Light_Italic.ttf",
            "/Users/mimi/Documents/PORTFOLIO PROJECTS/TerraHaven/node_modules/@expo-google-fonts/lato/400Regular/Lato_400Regular.ttf",
            "/Users/mimi/Documents/PORTFOLIO PROJECTS/TerraHaven/node_modules/@expo-google-fonts/lato/400Regular_Italic/Lato_400Regular_Italic.ttf",
            "/Users/mimi/Documents/PORTFOLIO PROJECTS/TerraHaven/node_modules/@expo-google-fonts/lato/700Bold/Lato_700Bold.ttf",
